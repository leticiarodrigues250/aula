lista_nome = []
lista_cpf = []

for i in range(0 , 5):
    controle = 's'
    nome = input('Nome: ')
    cpf = input('CPF: ')

    if cpf in lista_cpf:
        print('CPF já existente')
    else:
        lista_nome.append(nome)
        lista_cpf.append(cpf)

print(lista_cpf)
print(lista_nome)


dicionario_nome = {}

for i in range(0 , 5):
    nome = input('Nome: ')
    cpf = input('CPF: ')

    if cpf in dicionario_nome.values():
        print('CPF já existente')
    else:
        dicionario_nome[nome] = cpf
        print(dicionario_nome)
        
        
