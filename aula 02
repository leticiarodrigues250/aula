class Pessoa:
    def __init__(self, nome, idade, altura, peso):
        self.nome = nome
        self.idade = idade
        self.altura = altura
        self.peso = peso

    def engordar(self, kg):
        self.peso = self.peso + kg
    def emagrecer(self, kg):
        self.peso = self.peso - kg
    def crescer(self, cm):
        self.altura = self.altura + cm
    def envelhecer(self):
        if self.idade < 21:
            self.altura = self.altura + 0.5
        self.idade = self.idade + 1

pessoa = Pessoa("Leticia", 18, 170, 55 )

pessoa.envelhecer()
pessoa.envelhecer()
pessoa.envelhecer()
pessoa.envelhecer()
pessoa.envelhecer()
print(pessoa.idade)
print(pessoa.altura)
